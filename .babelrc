{
    "ignore": [
        "node_modules/**"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-for-of",
            {
                "assumeArray": true
            }
        ],
        "@babel/plugin-transform-object-assign"
    ],
    "presets": [
        [
            "@babel/env",
            {
                "modules": false
            }
        ]
    ]
}
